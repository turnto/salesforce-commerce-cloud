    "istanbul": "^0.4.5",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^2.7.6",
    "minimist": "^1.2.6",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "moment-timezone": "^0.5.26",
    "nyc": "^15.1.0",
    "postcss-loader": "^7.0.0",
    "prettier": "3.0.0",
    "properties-parser": "^0.3.1",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.6",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.2",
    "selenium-standalone": "^9.3.1",
    "sgmf-scripts": "^3.0.0",
    "shelljs": "^0.8.5",
    "should": "^13.2.3",
    "sinon": "^17.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^15.4.0",
